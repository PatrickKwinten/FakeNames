Use "PolicyManagement"

</lotusscript></code></globals><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	Dim Policy As NotesDocument
	
	Set Policy = Source.Document
	
	MailFormat = Policy.GetItemValue("MIMEMail")
	NewsFormat = Policy.GetItemValue("MIMENews")
	Select Case MailFormat(0)
	Case "0" :   
		Select Case NewsFormat(0)
		Case "0" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "17") 
		Case "1" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "33")
		Case "2" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "65")
		Case "3" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "129")
		End Select
	Case "1" : 
		Select Case NewsFormat(0)
		Case "0" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "18")
		Case "1" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "34")
		Case "2" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "66")
		Case "3" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "130")
		End Select
	Case "2" : 
		Select Case NewsFormat(0)
		Case "0" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "20")
		Case "1" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "36")
		Case "2" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "68")
		Case "3" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "132")
		End Select
	Case "3" : 
		Select Case NewsFormat(0)
		Case "0" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "24")
		Case "1" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "40")
		Case "2" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "72")
		Case "3" : Call Policy.ReplaceItemValue("$Pref$MIMESaveFormat", "136")
		End Select
	End Select
	
	Dim dcItem As notesitem
	Dim listItems As NotesItem
	
'	Set listItems = Policy.GetFirstItem("DontChangeItems")
'	If Not listItems Is Nothing Then
'		Forall c In listItems.Values
'			If Not c = "" Then
'				Set dcItem = Policy.GetFirstItem(c)
'				If Not dcItem Is Nothing Then
'					dcItem.SaveToDisk=False
'				End If
'			End If
'		End Forall	
'	End If
	
End Sub</lotusscript></code>
