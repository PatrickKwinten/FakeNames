<pardef id='246' hide='web mobile' keepwithnext='true' keeptogether='true'><code
 event='hidewhen'><formula>@IsAvailable($PrefCHECK_QUOTA_ON_MAIL_CREATE) &amp; $PrefCHECK_QUOTA_ON_MAIL_CREATE = "1" | !@Environment("SetCustomINI") = "1"</formula></code></pardef>
<par def='246'><run><font size='9pt'/></run><button width='2in' widthtype='maximum'
 maxlines='1' wraptext='true' bgcolor='system'><font size='9pt'/>Set Custom INI<code
 event='declarations'><lotusscript>


</lotusscript></code><code event='click'><lotusscript>Sub Click(Source As Button)
	
	On Error Goto ERROR_HANDLER
	
	Dim workspace As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim vSetIP As Variant
	Dim vSetPO As Variant
	
	Set uidoc = workspace.CurrentDocument
	uidoc.EditMode = True	
	Set doc = uidoc.Document
	vSetIP = False
	vSetPO = False
	
	Dim ApplyCustomINI(0 To 10, 0) As String
	ApplyCustomINI(0,0) = "$PrefCHECK_QUOTA_ON_MAIL_CREATE,1"
	ApplyCustomINI(1,0)= "$Pref$Reactivate_Mail_Quota,1"
	ApplyCustomINI(2,0)= "$PrefCheckQuotaWhenLessThan80,30"
	ApplyCustomINI(3,0)= "$PrefCheckQuotaWhenLessThan90,15"
	ApplyCustomINI(4,0)= "$PrefCheckQuotaWhenLessThan100,5"
	ApplyCustomINI(5,0)= "$Pref$CollapsePersonalGroups,1"
	ApplyCustomINI(6,0)= "$Pref$CS_RMF,1"
	ApplyCustomINI(7,0)= "$PrefCalendar_Multiline_Subject,1"
	ApplyCustomINI(8,0)= "$PrefPluginsWarningOption,1"
	ApplyCustomINI(9,0)= "$Pref$Convert_FollowUp,1"	
	ApplyCustomINI(10,0)= "$Pref$CustomExpirationEnabled,1"	
	
	If Not Doc Is Nothing Then			
		'// Check for inheritance 
		If doc.getitemvalue("SetCustomINI$IP")(0) = "1" Then vSetIP = True
		If doc.getitemvalue("SetCustomINI$PO")(0) = "1" Then vSetPO = True
		
		For i=0 To Ubound(ApplyCustomINI)
			'// Create items now....
			Call doc.replaceitemvalue(Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1), Mid(Trim(ApplyCustomINI(i, 0)), Instr(Trim(ApplyCustomINI(i, 0)), |,|) + 1))
			'// If inheritance has been set, create items now...
			If vSetIP Then Call doc.replaceitemvalue( Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1) + "$IP" , "1")				
			If vSetPO Then Call doc.replaceitemvalue( Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1) + "$PO" , "1")				
		Next		
	End If
	Call uidoc.Refresh
	
ROUTINE_EXIT:
	Exit Sub
	
ERROR_HANDLER:
	Messagebox "Set Custom INI button " &amp; Cstr(Err()) &amp; ": " &amp; Error &amp; " occurred on line " &amp; Cstr(Erl())
	
	Resume ROUTINE_EXIT
	
End Sub</lotusscript></code></button><run><font size='9pt'/></run></par>
<pardef id='247' hide='web mobile' keepwithnext='true' keeptogether='true'><code
 event='hidewhen'><formula>!@IsAvailable($PrefCHECK_QUOTA_ON_MAIL_CREATE) | $PrefCHECK_QUOTA_ON_MAIL_CREATE = "0"  | !@Environment("SetCustomINI") = "1"</formula></code></pardef>
<par def='247'><run><font size='9pt'/></run><button width='2in' widthtype='maximum'
 maxlines='1' wraptext='true' bgcolor='system'><font size='9pt'/>Clear Custom INI<code
 event='declarations'><lotusscript>Const DYC = "$Pref"

</lotusscript></code><code event='click'><lotusscript>Sub Click(Source As Button)
	
	On Error Goto ERROR_HANDLER
	
	Dim workspace As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim vSetIP As Variant
	Dim vSetPO As Variant
	Dim stName As String
	Dim stValue As String
	
	Set uidoc = workspace.CurrentDocument
	uidoc.EditMode = True	
	Set doc = uidoc.Document
	vSetIP = False
	vSetPO = False
	
	Dim ApplyCustomINI(0 To 10, 0) As String
	ApplyCustomINI(0,0) = "$PrefCHECK_QUOTA_ON_MAIL_CREATE,0"
	ApplyCustomINI(1,0)= "$Pref$Reactivate_Mail_Quota,0"
	ApplyCustomINI(2,0)= "$PrefCheckQuotaWhenLessThan80,30"
	ApplyCustomINI(3,0)= "$PrefCheckQuotaWhenLessThan90,15"
	ApplyCustomINI(4,0)= "$PrefCheckQuotaWhenLessThan100,5"
	ApplyCustomINI(5,0)= "$Pref$CollapsePersonalGroups,0"
	ApplyCustomINI(6,0)= "$Pref$CS_RMF,0"
	ApplyCustomINI(7,0)= "$PrefCalendar_Multiline_Subject,0"
	ApplyCustomINI(8,0)= "$PrefPluginsWarningOption,0"
	ApplyCustomINI(9,0)= "$Pref$Convert_FollowUp,0"	
	ApplyCustomINI(10,0)= "$Pref$CustomExpirationEnabled,0"	
	
	If Not Doc Is Nothing Then			
		'// Check for inheritance 
		If doc.getitemvalue("SetCustomINI$IP")(0) = "1" Then vSetIP = True
		If doc.getitemvalue("SetCustomINI$PO")(0) = "1" Then vSetPO = True
		
		For i=0 To Ubound(ApplyCustomINI)
			'// Create items now....
			Call doc.replaceitemvalue(Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1), Mid(Trim(ApplyCustomINI(i, 0)), Instr(Trim(ApplyCustomINI(i, 0)), |,|) + 1))
			
			'// If inheritance has been set, create items now...
			If vSetIP Then 
				Call doc.replaceitemvalue( Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1) + "$IP" , "1")				
			Else
				Call doc.replaceitemvalue( Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1) + "$IP" , "")				
			End If
			
			If vSetPO Then 
				Call doc.replaceitemvalue( Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1) + "$PO" , "1")				
			Else
				Call doc.replaceitemvalue( Left(ApplyCustomINI(i, 0), Instr(Trim(ApplyCustomINI(i, 0)), |,|) -1) + "$PO" , "")				
			End If
		Next		
	End If
	
	Call uidoc.Refresh
	
ROUTINE_EXIT:
	Exit Sub
	
ERROR_HANDLER:
	Messagebox "Clear Custom INI button " &amp; Cstr(Err()) &amp; ": " &amp; Error &amp; " occurred on line " &amp; Cstr(Erl())
	
	Resume ROUTINE_EXIT
	
End Sub</lotusscript></code></button><run><font size='9pt'/></run></par></tablecell>
