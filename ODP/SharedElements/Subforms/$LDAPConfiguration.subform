<par def='9'><run><font size='9pt'/></run><button width='2in' widthtype='maximum'
 maxlines='1' wraptext='true' bgcolor='system'><font size='9pt'/>Select Attribute Types<code
 event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim hDoc As NotesDocument
	Dim hWorkspace As NotesUIWorkspace 
	Dim hUidoc As NotesUiDocument
	Dim hItem As NotesItem
	
	Dim vVersion As Variant
	
	On Error Goto ERROR_HANDLER
	
	' if the version is earlier than Rnext, then bail
	vVersion = Evaluate(|@TextToNumber(@Version)|,doc)
	If vVersion(0) &lt; 171 Then
		Messagebox "You must use a Notes 6 client or later to configure LDAP Attribute Types."
		Exit Sub
	End If
	
	' set the dim'd variables
	Set hWorkspace = New NotesUIWorkspace 
	Set hUidoc = hWorkspace.CurrentDocument
	Set hDoc = hUidoc.Document
	Set hItem = hDoc.GetFirstItem("LDAPQueriableAttributes")
	
		' if the user hits "OK" (domino provided) then set the LDAPATACL field to the LDAPQueriableAttributes values
	If hWorkspace.Dialogbox( "(LDAPAttributeTypeList)", True, True, False, False, False, False, "LDAP Attribute Type Selection", , True, False, True ) Then
		If hDoc.LDAPQueriableAttributes(0) = "" Then
			Msgbox "You must choose at least one queriable LDAP attribute.",MB_OK+MB_ICONSTOP,"LDAP Attribute Selection Error"
			If hWorkspace.Dialogbox( "(LDAPAttributeTypeList)", True, True, False, False, False, False, "LDAP Attribute Type Selection", , True, False, True ) Then
				hDoc.LDAPATACL = hDoc.LDAPQueriableAttributes
				Call hUidoc.Refresh
			End If
		Else
			hDoc.LDAPATACL = hDoc.LDAPQueriableAttributes
			Call hUidoc.Refresh		
		End If
	End If
	
ROUTINE_EXIT:
	Exit Sub
	
ERROR_HANDLER:
	Msgbox "Internal Error - Subform:  Line# " &amp; Format$(Erl) &amp; "   Error: " &amp; Error$
	Resume ROUTINE_EXIT
End Sub</lotusscript></code></button><run><font size='9pt'/></run></par>
