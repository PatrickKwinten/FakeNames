<par def='73'><run><font size='9pt'/></run><button width='2in' widthtype='maximum'
 maxlines='1' wraptext='false' edge='rounded' bgcolor='system'><font size='8pt'/>Remove All<code
 event='options'><lotusscript>Option Explicit

</lotusscript></code><code event='declarations'><lotusscript>Const END_REMOVE_NOTIFICATION = "Trust Links Removed"

</lotusscript></code><code event='click'><lotusscript>Sub Click(Source As Button)
	Dim uiws As New NotesUIWorkspace,uidoc As NotesUIDocument,adminDoc As NotesDocument
	Dim rtLinksItem As NotesRichTextItem,newUIDoc As NotesUIDocument
	Set uidoc = uiws.CurrentDocument
	Set adminDoc= uidoc.Document
	adminDoc.saveoptions="0"
	Call adminDoc.RemoveItem("$NCrossLinks")
	Call adminDoc.RemoveItem("$ICrossLinks")
	Call adminDoc.RemoveItem("$ICertLinks")
	Call adminDoc.RemoveItem("$TrustLinkIds")
	Call admindoc.ReplaceItemValue("$dtLinksUpdate",Now)
	Call uidoc.Close(True)
	Set newUIDoc = uiws.EditDocument(True,adminDoc,False,,,True)
	Delete uiDoc
	Call newUIDoc.Document.ReplaceItemValue("SaveOptions","1")
	Call newUIDoc.Reload
	Print END_REMOVE_NOTIFICATION 
End Sub</lotusscript></code></button><run><font size='9pt'/></run></par></tablecell></tablerow></table>
<pardef id='74' leftmargin='0.0597in' hide='mobile' keepwithnext='true' keeptogether='true'><code
 event='hidewhen'><formula>@Elements($TrustLinkIds)=0</formula></code></pardef>
<section onread='collapse' onpreview='collapse' onedit='expand' expanded='true'><sectiontitle
 pardef='74' color='teal'><text>Certificate Links</text></sectiontitle>
<pardef id='75' leftmargin='0.0597in' hide='mobile' keepwithnext='true' keeptogether='true'/>
<par def='75'><run html='true'>&lt;div&gt;</run><run html='true'><computedtext
><code event='value'><formula>REM {DNT Begin};
slash:="/";
protocol:=@If(@ClientType= "Notes";"Notes://";"http://");
checkServer := @If(@ClientType= "Notes";@Name([CN]; @Subset(@DbName; 1));@GetHTTPHeader("Host"));
server:=@If(checkServer="";slash;checkServer+slash);
database := @Subset(@DbName; -1) + slash;
view:="Certificates"  + slash;
url:=protocol + server + database + view;

@For(i := 1; i &lt;= @Elements($TrustLinkIds); i := i + 1;
sNameCombo:=@Trim(@GetDocField($TrustLinkIds[i];"namecombo"));
IssuedToText:=@Name([Abbreviate];@If(sNameCombo="";@GetDocField($TrustLinkIds[i];"IssuedTo");sNameCombo));
linkText:=@If(@Length(IssuedToText)&gt;75;@Left(IssuedToText;75)+"...";IssuedToText);
HTML := HTML + "&lt;a href='" + url + $TrustLinkIds[i] + "'&gt;" + linkText + "&lt;/a&gt;";
HTML := HTML + "&lt;br&gt;");
REM {DNT End};
HTML</formula></code></computedtext></run><run html='true'>&lt;/div&gt;</run></par>
