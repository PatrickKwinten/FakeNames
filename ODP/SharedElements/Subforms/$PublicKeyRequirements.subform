<par def='72'><run><font size='9pt'/></run><button width='2in' widthtype='maximum'
 maxlines='1' wraptext='false' edge='rounded' bgcolor='system'><font size='8pt'/>Update Links<code
 event='options'><lotusscript>Option Explicit

</lotusscript></code><code event='declarations'><lotusscript>'Write valid certificates to trust links field: 

Const Err_View = "Required View not found: "
Const PROCESS_CANCELLED = "Process Cancelled"
Const ALL_SUPPORTED = "All supported"
Const SELECTED_SUPPORTED = "Selected supported"
Const SELECT_CERTIFICATES_PROMPT = "Select the certificates to trust"
Const PROMPT_TITLE = "Trusted Certificates"
Const BEGIN_COPY = "Copying trust documents"
Const END_COPY = "Completed copying trust documents"

'Begin DNT
Const ALLTRUSTEDCERTS = "(trustCertLkup)"
'END DNT

Dim unidStrings() As String

</lotusscript></code><code event='click'><lotusscript>Sub Click(Source As Button)
	Dim session As New NotesSession,thisDB As NotesDatabase, Certifiers As NotesView
	Dim uiws As New NotesUIWorkspace, adminDoc As NotesDocument,uidoc As NotesUIDocument,newUIDoc As NotesUIDocument
	Dim certDoc As NotesDocument
	Dim promptList(1) As String,updateType,isOK As Boolean
	promptList(0) = ALL_SUPPORTED
	promptList(1) = SELECTED_SUPPORTED
	
	Set uidoc = uiws.CurrentDocument
	Set adminDoc= uidoc.Document
	Set thisDB=session.CurrentDatabase
	
	'prompt to choose all or selected certs
	updateType = uiws.Prompt (PROMPT_OKCANCELLIST, PROMPT_TITLE,SELECT_CERTIFICATES_PROMPT,ALL_SUPPORTED,promptList)
	If Not Isempty (updateType) Then
		Print BEGIN_COPY
		If  updateType = ALL_SUPPORTED Then 'user chose to get all supported cert docs in certicates view
			Set Certifiers  = thisDB.GetView(ALLTRUSTEDCERTS)
			If Certifiers Is Nothing Then
				Print (Err_View &amp; ALLTRUSTEDCERTS)
				End
			End If		
			Set certDoc = Certifiers.GetFirstDocument
			While Not certDoc Is Nothing 
				Call addStringToArray(unidStrings(),certDoc.UniversalID)
				Set certDoc = Certifiers.GetNextDocument(certDoc)	
			Wend
			If unidStrings(0)&lt;&gt;"" Then
				adminDoc.saveoptions="0"
				admindoc.Form="PolicySecurity"
				Call admindoc.ReplaceItemValue("$dtLinksUpdate",Now)
				Call admindoc.ReplaceItemValue("$TrustLinkIds",unidStrings())
				Call uidoc.Close(True)
				Set newUIDoc = uiws.EditDocument(True,adminDoc,False,,,True)
				Delete uiDoc
				Call newUIDoc.Document.ReplaceItemValue("SaveOptions","1")
				Print END_COPY
			End If
		Else 'user chose to get selected docs in certicates view
			isOK= uiws.DialogBox("(CertListbox)",True,True,False,False,False,False,SELECT_CERTIFICATES_PROMPT,,True)
			If isOK Then
				adminDoc.saveoptions="0"
				admindoc.Form="PolicySecurity"
				Call admindoc.ReplaceItemValue("$dtLinksUpdate",Now)
				Call uidoc.Close(True)
				Set newUIDoc = uiws.EditDocument(True,adminDoc,False,,,True)
				Delete uiDoc
				Call newUIDoc.Document.ReplaceItemValue("SaveOptions","1")
			Else
				Print  PROCESS_CANCELLED
			End If
		End If
	Else 'cancelled dialog
		Print  PROCESS_CANCELLED
	End If
End Sub
</lotusscript></code><code event='initialize'><lotusscript>Sub Initialize
	Redim unidStrings(0) As String	
End Sub
</lotusscript></code><code event='addStringToArray'><lotusscript>Sub addStringToArray(allStrings() As String,newString As String)
	If Not (allStrings(Ubound(allStrings))="") Then		
		Redim Preserve allStrings(Ubound(allStrings)+1)
	End If
	allStrings(Ubound(allStrings))=newString
End Sub</lotusscript></code></button><run><font size='9pt'/></run></par></tablecell>
