	Dim s_alt$, s_val$
	s_alt = source.FieldGetText("iwaEnableAltNmPref")
	s_val = source.FieldGetText("iwaEnableNmVldtn")
	If Not (s_alt = "") Then
		If  (s_val = "") Then
			Messagebox L_Warning, MB_OK, L_Title
			Call source.FieldSetText( "iwaEnableNmVldtn", "1" )
