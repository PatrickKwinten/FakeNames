		Call doc.replaceitemvalue("bChatAlreadyConverted", 1)
	End If
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	Dim sChatval As String
	Dim doc As NotesDocument
	
	Set doc = Source.Document
	If Not Isnewdoc Then
		If Not doc.HasItem("bChatAlreadyConverted") Then
			sChatval = doc.GetItemValue("iwaCHAT")(0)
			If sChatVal = "0" Then
				Call doc.ReplaceItemValue("iwaChatEntry", "0")
			Else
				Call doc.ReplaceItemValue("iwaChatEntry", "1")
			End If
			Call doc.ReplaceItemValue("bChatAlreadyConverted", 1)
		End If
	End If
End Sub</lotusscript></code>
