<wassignedby><name>CN=Lotus Notes Template Development/O=Lotus Notes</name></wassignedby><code
 event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	Dim doc As NotesDocument
	Dim sSideBarVal As String
	Dim sSideBarUP As String
	Dim sSideBarIP As String
	Dim sSideBarPO As String
	
	Set doc = Source.Document
	If Not doc Is Nothing Then
		sSideBarVal = doc.GetItemValue("$DWAShowSidebar")(0)
		sSideBarUP = doc.GetItemValue("$DWAShowSidebar$UP")(0)
		sSideBarIP = doc.GetItemValue("$DWAShowSidebar$IP")(0)
		sSideBarPO = doc.GetItemValue("$DWAShowSidebar$PO")(0)
		
		If  sSideBarVal &lt;&gt; doc.GetItemValue("$INVWShowSidebar")(0) Then
			Call doc.replaceitemvalue("$INVWShowSidebar", sSideBarVal)
		End If
		If sSideBarUP &lt;&gt; doc.GetItemValue("$INVWShowSidebar$UP")(0) Then
			Call doc.ReplaceItemValue("$INVWShowSidebar$UP", sSideBarUP)
		End If
		If sSideBarIP &lt;&gt; doc.GetItemValue("$INVWShowSidebar$IP")(0) Then
			Call doc.ReplaceItemValue("$INVWShowSidebar$IP", sSideBarIP)
		End If
		If sSideBarPO &lt;&gt; doc.GetItemValue("$INVWShowSidebar$PO")(0) Then
			Call doc.ReplaceItemValue("$INVWShowSidebar$PO", sSideBarPO)
		End If
	End If
End Sub</lotusscript></code>
