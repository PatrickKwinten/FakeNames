<pardef id='24' align='center' spaceafter='1.5' hide='web' keepwithnext='true'
 keeptogether='true'><code event='hidewhen'><formula>INIFlg!="Add"</formula></code></pardef>
<par def='24'><run><font size='9pt' name='user-interface'/></run><button width='1.5000in'
 widthtype='fixed' wraptext='false' bgcolor='system'><code event='click'><formula
>@If(NewParameter="";@Return(@Prompt([Ok];"Error";"You must choose or enter a Context before using the 'Add/Modify' button."));"");
@If(NewValue="";@Return(@Prompt([Ok];"Error";"You must enter a URL value before using the 'Add/Modify' button."));"");

FIELD SetClearFlag:=@False;
FIELD CurrentParameter:=NewParameter;
REM {DNT Begin};
FIELD DUMMY_STRING:="**$$DUMMY$$**";

ParameterToUpdate:=NewParameter;
REM { This is used if only Context name needs to be unique
Position:=@Member(ParameterToUpdate;@Left(Parameters;"="));
};
CtxURL:=ParameterToUpdate + "=" + NewValue;
Position:= @Member(CtxURL;@Left(Parameters;";"));
Elements := @Elements(Parameters);
Bool := @If(Elements = Position; "L"; Position = 1; "F"; "M");
ListWithoutParameter := @If(Bool = "F" &amp; Elements &gt; 1; @Subset(Parameters; -(Elements - 1));Bool = "L" &amp; Elements = 1;""; Bool = "L"; @Subset(Parameters; (Elements - 1)); @Trim(@If(Position&gt;0;@Subset(Parameters; (Position - 1));"") : @Subset(Parameters; -(Elements - Position))));
FIELD Parameters:=@Trim(@Unique(ListWithoutParameter:(ParameterToUpdate+"="+NewValue+";"+NewActions+";"+NewCookies+";"+NewMIMETypes+";"+NewHeaders)));

