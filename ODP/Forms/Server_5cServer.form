<par def='326'><run><font size='9pt'/></run><button width='2in' widthtype='maximum'
 maxlines='1' wraptext='true' bgcolor='system'><font size='9pt'/>IdP Catalog<code
 event='click'><lotusscript>Sub Click(Source As Button)
	Dim ws As New NotesUIWorkspace
	Dim sess As New notessession
	Dim uidb As NotesUIDatabase
	Dim db As NotesDatabase
	Dim view As NotesView
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	Dim sServer As String
	Dim sPath As String
	Dim sName As String
	Dim sVal As String
	Dim sView As String
	Dim sHostname As String
	
	Set uidb = ws.CurrentDatabase
	Set db = uidb.Database
	sServer = db.Server
	sPath = db.FilePath
	sName = "idpcat.nsf"	
	sView = "($IdPConfigs)"
	Set uidoc = ws.currentdocument
	Set doc = uidoc.document
	sHostname = doc.GetItemValue("HTTP_HostName")(0)
	
	Set db = sess.getdatabase(sServer, sName, False)
	Set view = db.GetView(sView)
	Set doc = view.getdocumentbykey(sHostname, True)
	
	If doc Is Nothing Then
		Call ws.OpenDatabase(sServer, Strleft(sPath, "names.nsf") + sName,,,True)
	Else
		Set uidoc = ws.EditDocument(True, doc)
	End If
End Sub</lotusscript></code></button></par></tablecell></tablerow></table>
