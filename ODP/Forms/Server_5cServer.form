 event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Dim doc As NotesDocument
	Dim item As NotesItem
	Dim boxtype As Long
	Dim answer As Integer
	
	Set doc = source.Document
	
' This is used to force the onExiting event on the AccessServerInput field
	v = Evaluate( |@Version| )
	versionNumber = Format(v(0), "Fixed")
	
	If ( versionNumber &lt; 171 ) Then
		Call source.Document.ReplaceItemValue ("$servermain", 0)
		Call source.refresh
	End If
	
	Call source.GotoField("ServerName")
	
	Set item = doc.GetFirstItem("AllowAccess")
	
	REM { This is needed so that the AllowAccess field will recompute- now obsoleted by gotofield call above}
'	source.Refresh
	
	If doc.AllowAccess(0) = "*" Then
		If (Ubound(item.values) &lt; 1) Then
			boxtype = 35 'MB_YES + MB_ICONQUESTION 
			answer = Messagebox("You have not listed any servers in the Access Server field for this server.  No servers will be able to access this server.  Are you sure that you want to save this document?", boxtype, "Continue?")
			If answer=7Or answer=2 Then
				Continue=False
			End If
		End If
	End If
	
	If doc.getitemvalue("DAOSEnable")(0) = "1" Then
		If Not doc.GetItemValue("SHARED_MAIL")(0) = "0" Or doc.GetItemValue("TRANSLOG_Status")(0) = "0" Then
			If Not doc.HasItem("DAOSWarned") Or Not doc.GetItemValue("DAOSWarned")(0) = "1" Then
				Messagebox "To store new attachments in DAOS, you must enable Transaction Logging and set Shared Mail to None and save.  With current settings, DAOS will not be used to store attachments for Notes databases linked to Shared Mail (SCOS) but can continue to retrieve existing attachments from the DAOS repository for those databases. To use DAOS with any database currently using Shared Mail, unlink the database using the server command 'object unlink &lt;database&gt;' command.", 48, "Warning"		
				Call doc.replaceitemvalue("DAOSWarned", "1")
			End If
		Else
			Call doc.ReplaceItemValue("DAOSWarned","0")
		End If
	End If
	
	If doc.GetItemValue("bOutlookSupport")(0) = "1" Then
		Call doc.ReplaceItemValue("tsExternalURL", "https://" + doc.GetItemValue("ServerName")(0) + "/traveler")
		Call doc.replaceitemvalue("tsUserWipe", "0")
	End If
	
End Sub
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
	Dim doc As Notesdocument
	Dim sess As New Notessession
	Dim sOutlookEnv As String
	
	Set doc = Source.Document
	If doc.Isnewnote Then
		Call doc.Replaceitemvalue("FltRcvryNot", sess.Getenvironmentstring("Admin", True))
	End If
	
	sOutlookEnv = sess.GetEnvironmentString( "OutlookEnv", True )
	If Not sOutlookEnv = "" Then
		If Not (doc.GetItemValue("$ServerMain")(0) = 0) Then
			Call doc.ReplaceItemValue("$ServerMain", 0)
		End If
		Call doc.ReplaceItemValue("bOutlookSupport", sOutlookEnv)
		Call Source.RefreshHideFormulas
	End If
	
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	Dim doc As NotesDocument
	
	Set doc = Source.Document
	If Not doc Is Nothing Then
		If Doc.GetItemValue("WhiteList")(0) = "No" Then
			Call doc.ReplaceItemValue("WhiteList", "0")
		End If
	End If
	
End Sub
</lotusscript></code><code event='postmodechange'><lotusscript>Sub Postmodechange(Source As Notesuidocument)
	Dim sess As New NotesSession	
	Dim sOutlookEnv As String
	Dim doc As NotesDocument
	
	If Source.EditMode = True Then
		Set doc = Source.document
		sOutlookEnv = sess.GetEnvironmentString( "OutlookEnv", True )
		If Not sOutlookEnv = "" Then
			If Not (doc.GetItemValue("$ServerMain")(0) = 0) Then
				Call doc.ReplaceItemValue("$ServerMain", 0)
			End If
			Call doc.ReplaceItemValue("bOutlookSupport", sOutlookEnv)
			Call Source.Refresh
		End If
	End If	
End Sub</lotusscript></code><code event='onSubmit' for='web'><javascript>// do nothing in the Notes client
