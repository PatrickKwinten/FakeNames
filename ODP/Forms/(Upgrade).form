<globals><code event='options'><lotusscript>Option Public

</lotusscript></code><code event='declarations'><lotusscript>Dim w As NotesUIWorkspace
Dim s As NotesSession

Dim db As NotesDatabase
Dim doc As NotesDocument
Dim uidoc As notesuidocument</lotusscript></code></globals><code event='windowtitle'><formula
>@If(@IsNewDoc;"Upgrade Message - Administrator Setup";"Upgrade Message")</formula></code><code
 event='htmlbody'><formula>"topmargin=0 leftmargin=0 marginheight=0 marginwidth=0"</formula></code><code
 event='helprequest'><formula>@Command([OpenHelpDocument];[AdminHelp];"(Help)";"pubnames_f_Upgrade")</formula></code><code
 event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
     Set w = New NotesUIWorkspace 
     Set s = New NotesSession
     Set db = s.CurrentDatabase
     Set uidoc=source
     Set doc = uidoc.document
     
     Dim architecture As String
     
     architecture=Environ("PROCESSOR_ARCHITECTURE")
     If architecture= "ALPHA" Then
          doc.IsAlpha="1"
     Else
          doc.IsAlpha=architecture
     End If
     
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
     
     If Not IsNewDoc Then 
          
          Dim note As NotesDocument    
          Set note = source.Document
          
          If note.HasItem("DeliveredDate") And note.ConvertedDate(0) = "" Then
               
               Set s = New NotesSession
               Set db = s.CurrentDatabase
               Set view = db.GetView("($Profiles)")
               
               ' There is no $Profiles view, exit.
               If view Is Nothing Then Exit Sub
               
               source.AutoReload = False
               
               Dim profile As NotesDocument
               Set profile = view.GetDocumentByKey("Convert Profile")
               
               ' The user has a $Profiles view, but has not run CONVERT yet.
               If profile Is Nothing Then Exit Sub
               
               Call note.ReplaceItemValue("ConvertedDate", profile.ConvertedDate(0))
               Call note.Save (False, False)
               
          End If
     End If
End Sub</lotusscript></code>
