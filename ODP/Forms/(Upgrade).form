<par def='13'><run><font size='9pt'/></run><button width='2.1000in' widthtype='fixed'
 wraptext='true' bgcolor='system'><code event='click'><formula>V4 :=136;
V45:=145;		REM {mail45.ntf};
V46 :=147;		REM {mail46.ntf};
V5 :=166;		REM {mail50.ntf};
V6 :=171;		REM {mail6.ntf};


@If(@TextToNumber(@Version) &gt;= V4; @True; @Do(@Prompt([Ok];"Not Release 4"; "You must first Install Notes Release 4."); @Return(@False)));

mailDB := @MailDbName;

DbName:=@DbName;


@If(@UpperCase(@Implode(DbName)) = @UpperCase(@Implode(mailDB)); @True; @If(@Prompt([YesNo];"Convert Utility"; "The Convert utility will upgrade the mail file specified in your current Location record, which is not the same as the database currently in use. Do you want to proceed?"); @True; @Return(@False)));

server := @Subset(mailDB; 1);

serverName := @If(server = ""; ""; server + "!!");


file := @If( @Subset(mailDB; -1) = ""; mailDB; @Subset(mailDB; -1));


Plat:=@Platform;
oldTemplate := @If(OldTemplate = ""; "*"; OldTemplate);
ver:=@TextToNumber(@Version);
newTemplate := @If(NewTemplate = "";@If(ver &lt; 145;"mail4.ntf";@If (ver = 145; "mail45.ntf"; @If(ver = 147; "mail46.ntf";@If( ver = 166;"mail50.ntf";"mail6.ntf")))); NewTemplate);

REM {Command line switches.  Don't add a switch unless the convert tool with this version of Notes supports the switch.};

SlashlSwitch := @If(SlashI = "1"; "-i"; "");
FolderReferencesSwitch := @If(FolderReferences = "1" &amp; ver &gt;= 147 &amp; ver &lt; 171; " -m"; FolderReferences = "1" &amp; ver &gt;= 171; " -e";"");
CustomFolderSwitch:=@If (CustomFolders = "1" &amp; ver &gt;= 171; " -u";"");
switches := SlashlSwitch + FolderReferencesSwitch + CustomFolderSwitch;

notify:=@If(NotifyAdmin="1";"-a\""+From+"\"";"");
param := switches + " " + notify + " " +"\""+serverName + file +"\""+ " "+ oldTemplate + " " + "\""+serverName + newTemplate+"\"";
paramNoUpgrade := SlashlSwitch + FolderReferencesSwitch + " " + notify + " " +"\""+serverName + file +"\""+ " "+ oldTemplate + " " + "\""+serverName + newTemplate+"\"";

@If(Plat="Macintosh";@Do(
                                                @SetEnvironment("V4ConvertNewTemplate";newTemplate);
                                                @SetEnvironment("V4ConvertOldTemplate";oldTemplate);
                                                @SetEnvironment("V4ConvertSwitches";switches)
                                                       );
          "");
@Command([FileCloseWindow]);
@Command([FileCloseWindow]);

program := @If(Plat="Windows/32"; "n"; Plat="Windows/16";"_";Plat="OS/2v2";"i";Plat="Macintosh";"";Plat="UNIX";""; "") + "convert";

@If(PromptFolder!="1";
	@PostedCommand([Execute];program; param);
	@If(@Prompt([YesNo];"Upgrade design of custom folders?";"Do you want to upgrade the design of your custom folders?");
		@PostedCommand([Execute];program; param);
			@PostedCommand([Execute];program; paramNoUpgrade)));
@True</formula></code><font size='9pt'/>Upgrade My Mail File</button></par>
