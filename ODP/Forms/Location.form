 event='declarations'><lotusscript>Const xxWizardTitle="Configuring server connection"

Dim db As NotesDatabase
Dim uidoc As NotesUIDocument
Dim  doc As NotesDocument
Dim  newdoc As NotesDocument
Dim ItemDefaultPassthruServerName As NotesItem
Dim ItemLocationName As NotesItem
Dim phonenumber As Variant

</lotusscript></code><code event='click'><lotusscript>Sub Click(Source As Button)
	Dim w As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim  doc As NotesDocument
	Dim ItemDefaultPassthruServerName As NotesItem
	Dim ItemLocationName As NotesItem
	Dim ItemLocationType As NotesItem
	On Error Goto errhandle
	
	Set uidoc=w.CurrentDocument
	Set doc=uidoc.Document 
	If uidoc.editmode Then Call uidoc.Refresh
	
	Set ItemLocationType=doc.GetFirstItem("LocationType")
	Set ItemLocationName=doc.GetFirstItem("Name") 
	Set ItemDefaultPassthruServerName=doc.GetFirstItem("DefaultPassthruServer") 
	
	If Len(ItemLocationName.Text) = 0 Then
		Msgbox "Please enter a Location name"
		Exit Sub
	End If
	
%REM	
	If Len(ItemDefaultPassthruServerName.Text) = 0 Then 
		Msgbox "Please enter name for Passthru server"
		Exit Sub			
	End If
%END REM
                        'bring location doc into edit mode
	If Not(uidoc.editmode) Then uidoc.editmode = True	
	
	If ItemLocationType.Text="1" Then	
		Call w.DialogBox("($NotesDirectDialup Wizard)",True,True,True,True,,,xxWizardTitle,,True,True)
	Elseif ItemLocationType.Text="0" Then	
		Call w.DialogBox("($LAN Wizard)",True,True,True,True,,,xxWizardTitle,,True,True)		
	Elseif ItemLocationType.Text="2" Then		
		options=Evaluate("'LAN':'Notes Direct Dialup':'Network Dialup'")
		choice=w.Prompt(prompt_okcancellist, "Connection configuration", "In a Custom location, you can use any type of connection document. Please select the type of connection to create.", "LAN", options)		
		If choice="LAN" Then
			Call w.DialogBox("($LAN Wizard)",True,True,True,True,,,xxWizardTitle,,True,True)		
		Elseif choice="Notes Direct Dialup" Then
			Call w.DialogBox("($NotesDirectDialup Wizard)",True,True,True,True,,,xxWizardTitle,,True,True)		
		Elseif 	choice="Network Dialup" Then
			     'check if DUN installed, if not -exit sub
			vList = w.Get_DialEntry_List("RAS")
			Call w.DialogBox("($NetworkDialup Wizard)",True,True,True,True,,,xxWizardTitle,,True,True)		
		End If
	Elseif  ItemLocationType.Text="4" Then
                        'check if DUN installed, if not -exit sub
		vList = w.Get_DialEntry_List("RAS")
		
                          'bring wizard and create RAS and/or Passthru connections based on Location Type
		Call w.DialogBox("($NetworkDialup Wizard)",True,True,True,True,,,xxWizardTitle,,True,True)
	End If
'	End If
	If uidoc.editmode Then
		uidoc.Save 
'		uidoc.editmode = False		
	End If
	Exit Sub
	
errhandle:
	If Str(Err)="-3878" Then Msgbox "You don't have Dialup Networking installed."
	Exit Sub
End Sub</lotusscript></code><code event='hidewhen'><formula>@IsNotMember(LocationType;"1":"4":"0":"2") | @Begins(@Platform;"Macintosh")</formula></code></action>
