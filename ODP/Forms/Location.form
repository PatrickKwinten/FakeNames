LocationType</formula></code><code event='onchange'><lotusscript>Sub Onchange(Source As Field)
	Dim uiws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	
	Set uidoc = uiws.CurrentDocument
	Set doc = uidoc.Document
	If doc.GetItemValue("LocationType")(0) = "3" Then 
		If doc.GetItemValue("ReplicationEnabled")(0) = "1" Then
			Call doc.ReplaceItemValue("repenbld", "1")
			Call doc.replaceitemvalue("ReplicationEnabled", "0")
			Call doc.ReplaceItemValue("oldloctype", "3")
		End If
	Elseif doc.GetItemValue("oldloctype")(0) = "3" Then
		If doc.GetItemValue("repenbld")(0) = "1" Then
			Call doc.ReplaceItemValue("ReplicationEnabled", "1")
			Call doc.ReplaceItemValue("repenbld", "0")
			Call doc.ReplaceItemValue("oldloctype", LocationType)
		End If
	End If
End Sub</lotusscript></code></field></run></par></tablecell>
