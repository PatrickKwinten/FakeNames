 hotspotstyle='none'><code event='click'><lotusscript>Sub Click(Source As Button)
	Dim w As New NotesUIWorkspace
	Dim s As New NotesSession
	Dim db As NotesDatabase
	Dim uidoc As NotesUIDocument
	Dim  doc As NotesDocument
	Dim  newdoc As NotesDocument
    Dim ItemDefaultPassthruServerName As NotesItem
    Dim ItemLocationName As NotesItem
	Dim phonenumber As Variant
	
	Set db=s.CurrentDatabase
	Set uidoc=w.CurrentDocument
	Set doc=uidoc.Document	

	Set ItemDefaultPassthruServerName=doc.GetFirstItem("DefaultPassthruServer")	

	If Len(ItemDefaultPassthruServerName.Text) = 0 Then
		Msgbox "Please enter name for Passthru server"
	Else	
    Set ItemLocationName=doc.GetFirstItem("Name")	
	If Len(ItemLocationName.Text) = 0 Then
			Msgbox "Please enter a Location name"
	Else
		Set newdoc=New NotesDocument(db)	
		Call newdoc.ReplaceItemValue("Form","Connection")
		Call newdoc.ReplaceItemValue("ConnectionType","1")
		Call ItemDefaultPassthruServerName.CopyItemToDocument(newdoc,"Destination")		
		Call ItemLocationName.CopyItemToDocument(newdoc,"ConnectionLocation")
		phonenumber=w.Prompt(Prompt_edit, "Phone Number", "Please enter a phone number","" )

		Call newdoc.ReplaceItemValue("LocalPhoneNumber", phonenumber)
		
		success= newdoc.ComputeWithForm(False, False)	
		
		If success Then
			Call newdoc.save(True, True)
		Else
			Msgbox("There are Input translation/validation issues") 	
		End If
	End If
End If
End Sub</lotusscript></code></actionhotspot><run><font size='2pt'/></run></par></tablecell></tablerow>
