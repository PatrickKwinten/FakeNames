 event='postsave'><formula>@Command([SetCurrentLocation];"")</formula></code><code
 event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	Dim w As New NotesUIWorkspace 
	Dim s As New NotesSession
	Dim db As NotesDatabase
	Dim doc As NotesDocument
	Set db = s.CurrentDatabase
	Dim uidoc As notesuidocument
	Set uidoc=w.currentdocument
	Set doc = uidoc.document
	Dim PkName As NotesName
	Dim userName As NotesName
	
	If doc.ProxyFlag(0) = "1" Then
		doc.Proxy_FTP=doc.Proxy_HTTP(0)
		doc.Proxy_Gopher=doc.Proxy_HTTP(0)
		doc.Proxy_SSL=doc.Proxy_HTTP(0)
	End If
	
	If doc.GetItemValue("Proxy_Password")(0) &lt;&gt; "" Or doc.GetItemValue("SametimeProxyPassword")(0) &lt;&gt; "" Then
		Set PkName = New NotesName(doc.GetItemValue("ProxyAuthor")(0))
		Call doc.ReplaceItemValue("PublicEncryptionKeys",PkName.Abbreviated)
	End If
	
	ImailAddr = doc.GetItemValue("ImailAddress")(0)
	If ( ImailAddr &lt;&gt; strImailAddress) Then
		Set userName = New NotesName(s.UserName)
		Call doc.ReplaceItemValue("ImailAddressUpdatedBy", userName.Canonical)
		strImailAddress = ImailAddr
'		Call doc.save(True, False)
	End If
	
	Dim flagHasSTproxyPassword As Boolean
	flagHasSTproxyPassword = False
	If doc.getitemvalue("SametimeProtocol")(0) = "3" And doc.getitemvalue("SametimeProxyType")(0) &lt;&gt; "0" Then
		flagHasSTproxyPassword = True
	Else
		Call doc.replaceitemvalue("SametimeProxyUsername", "")
		Call doc.replaceitemvalue("SametimeProxyPassword", "")		
	End If 
	
	If doc.getitemvalue("ProxyAuthenticationFlag")(0) = "" Then
		Call doc.replaceitemvalue("Proxy_LoginName", "")
		Call doc.replaceitemvalue("Proxy_Password", "")		
		Set flagItem=doc.getfirstitem("PublicEncryptionKeys")
		If Not flagitem Is Nothing Then
			Call doc.removeitem("PublicEncryptionKeys")
		End If
	End If
	
End Sub
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
	Dim item As NotesItem
	Set doc = source.document
	
	Set item = doc.GetFirstItem("ImailAddress")
	strImailAddress = item.Text
	Call doc.replaceitemvalue("oldloctype", LocationType)
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
