<action title='Modify CA Configuration' showinmenu='false' hide='preview read previewedit web'><code
 event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim ws As New NotesUIWorkspace
	Dim doc As NotesDocument
	Dim dialogdoc As NotesDocument
	Dim CAAItem, RAItem, CANameItem As NotesItem
	
	Set dialogdoc = New NotesDocument(ws.currentdatabase.database)
	
	Set doc = ws.currentdocument.document
	
	Set CAAItem = doc.GetFirstItem(ITEMNAME_CAADMINS)
	Set RAItem = doc.GetFirstItem(ITEMNAME_RAS)
	Set CANameItem = doc.GetFirstItem(ITEMNAME_ISSUEDTO)
	
	Call CAAItem.CopyItemToDocument(dialogdoc, ITEMNAME_CAALIST)
	Call RAItem.CopyItemToDocument(dialogdoc,ITEMNAME_RALIST)
	Call CANameItem.CopyItemToDocument(dialogdoc, ITEMNAME_CANAME)
	
	'Dialogbox(byval formname as string, optional byval autohorzfit, optional byval autovertfit, optional byval nocancel,
	' optional byval nonewfields, optional byval nofieldupdate, optional byval readonly, optional byval title as string,
	' optional byval notesdocument, optional byval sizetotable, optional byval nookcancel, optional byval okcancelatbottom) as variant
	Call ws.DialogBox(FORMNAME_MODCSS,True, True, True,_
	False, False, False ,	 TITLE_MODCSSDIALOG,_
	dialogdoc,	True, True, False)
	
End Sub</lotusscript></code><code event='hidewhen'><formula>CAEnabled = "0" |
(@Member(@UserName;CAAdministrators) = 0)</formula></code></action>
<action title='Enable for CA Process' showinmenu='false' hide='preview read previewedit web'><code
 event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim ws As New NotesUIWorkspace
	Dim doc As NotesDocument
	Dim uidoc As NotesUIDocument
	
	Set doc = ws.CurrentDocument.document
	Call doc.ReplaceItemValue(ITEMNAME_CAENABLED, "1")
	Call doc.save(True, True)
	
	Msgbox("CA Process is now enabled.")
	
End Sub</lotusscript></code><code event='hidewhen'><formula>CAEnabled = "1"  |
(@Member(@UserName;CAAdministrators) = 0)</formula></code></action>
