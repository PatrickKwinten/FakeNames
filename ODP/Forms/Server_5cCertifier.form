<action title='Enroll CA in Hierarchy' showinbar='false' hide='preview read previewedit web'><code
 event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim ws As New NotesUIWorkspace
	Dim doc As NotesDocument
	Dim dialogdoc As NotesDocument
	Dim NameItem As NotesItem
	
	Set dialogdoc = New NotesDocument(ws.currentdatabase.database)
	
	Set doc = ws.currentdocument.document
	
	Set NameItem = doc.GetFirstItem(ITEMNAME_ISSUEDTO)
	Call NameItem.CopyItemToDocument(dialogdoc, ITEMNAME_SUBJECTNAME)
	
	'Dialogbox(byval formname as string, optional byval autohorzfit, optional byval autovertfit, optional byval nocancel,
	' optional byval nonewfields, optional byval nofieldupdate, optional byval readonly, optional byval title as string,
	' optional byval notesdocument, optional byval sizetotable, optional byval nookcancel, optional byval okcancelatbottom) as variant
	Call ws.DialogBox(FORMNAME_ENROLLCA,True,True,True,_
	False, False, False ,TITLE_ENROLLCADIALOG,_
	dialogdoc,	True, True)
	
End Sub</lotusscript></code><code event='hidewhen'><formula>CAEnabled = "0" |
(@Member(@UserName;RegistrationAuthorities) = 0)</formula></code></action>
<action title='Create Cross Certificate' showinbar='false' hide='preview previewedit web'><code
 event='click'><formula>subs:=@X509Certificates([Subject];UserCertificate;Certificate_NoEnc);
leftsubs:=@Left(subs;"|");
rightsubs:=@Right(subs;"|");
x := @Prompt([OkCancelListMult]; "Create Cross Certificate"; "What certificate do you want to cross certify?"; ""; leftsubs);
@If(x=@False | x="";@Return("");"");
loc:=@Member(x;leftsubs);
num:=@Elements(leftsubs);
loc2:=@Subset(@Subset(rightsubs;loc-num-1);1);
@Command([CreateCrossCert];UserCertificate;Certificate_NoEnc;loc2;"1")</formula></code></action>
<action title='Export Internet Certificates' showinbar='false' hide='preview previewedit web'><code
 event='click'><formula>subs:=@X509Certificates([Subject];UserCertificate;"");
leftsubs:=@Left(subs;"|");
rightsubs:=@Right(subs;"|");
x := @Prompt([OkCancelList]; "Export Internet Certificate"; "What certificate do you want to export?"; ""; leftsubs);
@If(x=@False | x="";@Return("");"");
loc:=@Member(x;leftsubs);
num:=@Elements(leftsubs);
loc2:=@Subset(@Subset(rightsubs;loc-num-1);1);
@Command([PKCS12ExportCertsFromNAB];UserCertificate;"";loc2;"1")</formula></code></action>
