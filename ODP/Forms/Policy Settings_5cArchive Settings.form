>Sub Click(Source As Button)
	
	'//	Save the current document
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Set uidoc=ws.CurrentDocument
	Dim doc As NotesDocument
	Set doc=uidoc.Document
	
	'//	Set the ACPFlag field to display appropriate OK on dialog	
	If doc.IsNewNote Then
		Call uidoc.FieldSetText("ACPFlg","add")
'		Call uidoc.Save
	Else
		uidoc.EditMode=True
		Call uidoc.FieldSetText("ACPFlg","add")
		Call uidoc.FieldSetText("setACPFlg","0")
	End If
	
	'//	Create a new UI doc using the dialog form, stamping the UNID in
	Call ws.DialogBox("($ACPDialog)",True, True, True, False, False, False, "Archive Criteria", ,True,True)
	
'	Call PopulateACPField(uidoc, doc)
	
'	Call uidoc.Refresh
'	Call uidoc.Save
	Call uidoc.Reload
	
	Call uidoc.Refresh	
End Sub</lotusscript></code></button></par></tablecell>
