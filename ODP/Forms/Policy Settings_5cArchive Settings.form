<globals><code event='options'><lotusscript>Option Public
Use "PolicyManagement"
</lotusscript></code><code event='PopulateACPField'><lotusscript>Sub PopulateACPField(uidoc As NotesUIDocument, doc As NotesDocument)
	If uidoc.FieldGetText("setACPFlg") = "0" Then
		doc.ACP=""
		doc.ACPDsp=""    
		Call uidoc.FieldSetText("setACPFlg","1")     
		Dim s As New NotesSession
		Dim db As NotesDatabase
		Dim view As NotesView
		Dim acpDoc As NotesDocument
		Dim collection As NotesDocumentCollection
		Dim item As NotesItem
		Dim dspItem As NotesItem
		
		Set item=doc.GetFirstItem("ACP")
		Set dspItem = doc.GetFirstItem("ACPDsp")
		Set db=s.CurrentDatabase
		Set view=db.GetView("($ACPEmbedded)")
		Call view.Refresh
		Set collection=view.GetAllDocumentsByKey(doc.DocumentID(0))
		Set acpDoc=collection.GetFirstDocument
		While Not acpDoc Is Nothing
			Call item.AppendToTextList(Cstr(acpDoc.UniversalID))
			Call dspItem.AppendToTextList(acpDoc.FullName(0))
			Set acpDoc=collection.GetNextDocument(acpDoc)
		Wend
	End If
	doc.ACPDsp=""
End Sub</lotusscript></code></globals><code event='windowtitle'><formula>@If(@IsNewDoc; "New Archiving Settings"; "Archiving Settings: " + FullName)</formula></code><code
