	"&lt;script src='/" + _tDb + "/WebJSNavigationTools5?OpenPage'&gt;&lt;/script&gt;"
)</formula></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	Dim doc As NotesDocument
	Dim item As Notesitem
	Set doc = source.document
	Dim userNm, userPwd As String
	
		' Need to keep the fields in synch
	Call doc.ReplaceItemValue("wDCLoc", doc.GetItemValue("DCLoc")(0))
	
	If doc.GetItemValue("SMTPRBLSup")(0) = "1" And doc.GetItemValue("SMTPRBLSites")(0) = "" Then
		boxtype = 16 'MB_OK + MB_ICONSTOP
		answer = Messagebox("You have  DNS Blacklist filters enabled and have not selected any DNS BlackList sites. You must either add a DNS BlackList site or disable DNS Blacklist filters", boxtype, "Error")
		Continue = False
	Else 
		Continue = True
	End If
	
	If (doc.GetItemValue("JrnlEnbld")(0) = "1" And doc.GetItemValue("JrnlMthd")(0) = "1" And doc.GetItemValue("JrnlCert")(0) = "") Then
		boxtype = 48 ' 0 + 48 = MB_OK + MB_ICONEXCLAMATION
		Messagebox("Warning: In order to properly encrypt the journal, you should enter the name of the person whose public key should be used to encrypt the messages.")
	End If
	
	If source.fieldgettext("ServerName") &lt;&gt; "*" Then
		Call marktempitems( doc )
	End If
	
	If doc.GetItemValue("SUGMaxUser")(0) = ""   Then
		Set item = doc.GetFirstItem("SUGMaxUser")
		item.savetoDisk = False
	End If
	
	If doc.GetItemValue("DCLoc")(0) = ""   Then
		Set item = doc.GetFirstItem("DCLoc")
		item.savetoDisk = False
	End If
	
	If doc.GetItemValue("SUGEnabled")(0) = "" Then
		If doc.HasItem("SUGEnabled") Then
			Set item = doc.GetFirstItem("SUGEnabled")
			item.savetoDisk = False
		End If
		Set item = doc.GetFirstItem("SUGMaxUser")
		item.savetoDisk = False
	End If
	
	userNm = doc.GetItemValue("MailRelaySMTPSenderAcctName")(0)
	userPwd = doc.GetItemValue("MailRelaySMTPSenderAcctPW")(0)
	If ( (userNm &lt;&gt; "" Or userPwd &lt;&gt;"") And Not (doc.HasItem("PublicEncryptionKeys") Or doc.HasItem("SecretEncryptionKeys")))  Then
		boxtype = 35 'MB_YES + MB_ICONQUESTION 
		answer = Messagebox("This note should be encrypted to protect the SMTP relay host account name and password. Are you sure that you want to save this document while it is unencrypted?", boxtype, "Continue?")
		If answer=7 Or answer=2 Then
			Continue=False
		End If
	End If
	
	If doc.GetItemValue("ProvEnbl")(0) = "1" And doc.GetItemValue("ProvUpdateSiteList")(0) = "" And doc.GetItemValue("ProvUseFeatureURL")(0) = "0" Then
		boxtype = 35 'MB_OK + MB_ICONSTOP
		answer = Messagebox("You have not selected any sites to include in search and have disabled the use of the update site specified in Feature. This combination will disable provisioning. Do you wish to continue?", boxtype, "Continue?")
		If answer =7 Or answer=2 Then
			Continue = False
		Else 
			Continue = True
		End If		
	End If
End Sub
</lotusscript></code><code event='MarkTempItems'><lotusscript>Sub MarkTempItems(doc As NotesDocument)     
	Dim itemlist As Variant
	On Error Resume Next
	itemlist = doc.Items
	Forall n In itemlist
		If Lcase(Left(n.Name, 4)) = "ldap" Then n.Savetodisk = False
		If Lcase(Left(n.Name, 2)) = "cm" Then 
			If Cstr(n.Values(0)) = "" Then n.savetodisk = False
		End If
	End Forall     
	
End Sub
</lotusscript></code><code event='postsave'><lotusscript>Sub Postsave(Source As Notesuidocument)
	
	Dim rti As NotesRichTextItem
	Dim rtnav As NotesRichTextNavigator
	Dim rtlink As NotesRichTextDoclink
	Dim doSave As Boolean
	Dim doc As NotesDocument
	Dim db As NotesDatabase	
	
	doSave = False
	Set rti = Source.Document.GetFirstItem("SmartUpdDBLink")
	Set rtnav = rti.CreateNavigator	
	Set rtlink = rtnav.GetFirstElement(RTELEM_TYPE_DOCLINK)
	Set doc = source.Document	
	
	If rtlink Is Nothing Then
		doc.removeitem("SmartUpdDBLink")
		doSave = True
	End If	
	
	If doc.GetItemValue("ILEnforce")(0) = "" Then
		Call doc.RemoveItem("ILMaxTries")			
		Call doc.RemoveItem("ILEnforce")			
		Call doc.RemoveItem("ILLogLockouts")			
		Call doc.RemoveItem("ILLogFailures")			
		Call doc.RemoveItem("ILExpNum")			
		Call doc.RemoveItem("ILTimeFormat")			
		Call doc.RemoveItem("ILExpMinutes")			
		Call doc.RemoveItem("ILClearNum")			
		Call doc.RemoveItem("ILTimeFormat2")			
		Call doc.RemoveItem("ILClearMinutes")					
		doSave = True
	End If
	
	If doSave = True Then
		Set db = doc.ParentDatabase	
		noteid = doc.NoteID	
		If  noteid &lt;&gt; 0 Then		
			Call doc.Save(True, False)		
			Delete doc
			Set doc = db.GetDocumentByID(noteid)
			If doc.HasItem("PublicEncryptionKeys") Or doc.HasItem("SecretEncryptionKeys") Then
				Call doc.Encrypt
			End If	
		End If		
		Call doc.Save(True,False)
	End If
End Sub
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
	Dim doc As NotesDocument
	
	Set doc = Source.Document
	If doc.Isnewnote = True Then
		Call doc.appenditemvalue("SUGEnabled", "0")
	End If
End Sub</lotusscript></code><code event='jsheader' for='web'><javascript>// ignore if not Domino 6
