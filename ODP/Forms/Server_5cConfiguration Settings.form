 event='onchange'><lotusscript>Sub Onchange(Source As Field)
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Set uidoc = ws.CurrentDocument
	
	Dim doc As NotesDocument
	Set doc = uidoc.Document
	
	Dim item As NotesItem
	Set item = doc.GetFirstItem("CalDtls")
	
	If doc.CalDtlsEnabled(0) = "1" Then
		Call item.AppendToTextList("AppointmentType")
		Call item.AppendToTextList("Categories")
		Call item.AppendToTextList("Chair")
		Call item.AppendToTextList("Location")
		Call item.AppendToTextList("Room")
	Else
		doc.CalDtls=""
	End If
	
	Call uidoc.Reload
	
End Sub</lotusscript></code></field></run><run><font size='9pt'/></run></par></tablecell></tablerow>
