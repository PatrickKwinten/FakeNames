</lotusscript></code><code event='CalcAcctName'><lotusscript>Sub CalcAcctName
	'Begin DNT
	Dim ws As New notesuiworkspace
	Dim doc As NotesDocument
	Dim uidoc As NotesUIDocument
	Dim RdmStr As String
	Dim ProtTypeStr As String
	Dim NameStr As String
	
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	
	If Not bLLN2=1 Then 
		RdmStr = Format(Now(),"General Number")
		ProtTypeStr = doc.GetItemValue("AccountType")(0)
		Select Case ProtTypeStr
		Case "Sametime": NameStr = sProtocols(1)
		Case "FileShare" : NameStr = sProtocols(2)
		Case "Connections" : NameStr = sProtocols(3)
		Case Else: NameStr = ProtTypeStr
		End Select
		
		Call doc.replaceitemvalue("AccountName", NameStr + RdmStr)
		Call doc.ReplaceItemValue("AccountDesc", "Account for: " + NameStr + RdmStr)
	End If
	'END DNT
End Sub

</lotusscript></code><code event='initialize'><lotusscript>Sub Initialize
	sProtocols(1) = "SameTime"
	sProtocols(2) = "Quickr"
	sProtocols(3) = "Connections"
End Sub
</lotusscript></code><code event='GenPropertiesList'><lotusscript>Sub GenPropertiesList(CurType As String)
	' Begin DNT
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	
	
	Set uidoc = ws.currentdocument
	Set doc = uidoc.Document
	
	Select Case CurType
	Case "Sametime":
		Dim propList(4) As String
		propList(1) = |providerId=Sametime|
		propList(2) = |partnerIdType=""|
		propList(3) = |type=Sametime|
		propList(4) = |hideinui=false|
		Call doc.ReplaceItemValue("Properties", propList())
	Case "FileShare":
		
	Case "Connections":
		Dim propList3(2) As String
		propList3(1) = "hideinui=false"
		propList3(2) = |primaryAccountName=Connections|
		Call doc.ReplaceItemValue("Properties", propList3())
	Case "Other":
		Dim propList4(1) As String
		propList4(1) = "hideinui=false"
		
		Call doc.ReplaceItemValue("Properties", propList4())
		
	Case Else:	
		Dim propList5(1) As String
		propList5(1) = "hideinui=false"
		
		Call doc.ReplaceItemValue("Properties", propList5())
	End Select
	
	Call uidoc.Refresh
	'END DNT
End Sub

</lotusscript></code><code event='UpdateAuthFields'><lotusscript>Sub UpdateAuthFields(CurType As String)
		' Begin DNT
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	
	' do nothing if this is lln2
	If bLLN2=1 Then 
		Return
	End If
	
	' do nothing if account type has not changed
	If CurType = sOriginalAccountType Then 
		Return
	End If
	
	Set uidoc = ws.currentdocument
	Set doc = uidoc.Document
	
	Select Case CurType
	Case "Sametime":
		Call doc.ReplaceItemValue("authtype", "ST-DOMINO-SSO")
		
	Case "Connections":
		Call doc.ReplaceItemValue("authtype", "J2EE-FORM")
		
	Case "Other":
		Call doc.ReplaceItemValue("authtype", "HTTP")
		
	Case "SAML-IdP":
		Call doc.ReplaceItemValue("authtype", "SAML-IdP")
		
	Case Else:	
		Call doc.ReplaceItemValue("authtype", "HTTP")
	End Select
	
	' remember the current value so we can detect a change in account type
	sOriginalAccountType = CurType
	
	'END DNT
End Sub
</lotusscript></code><code event='LinkAcct'><lotusscript>Sub LinkAcct
	Dim session As New NotesSession
	Dim thisDB As NotesDatabase
	Dim AccountVw As NotesView
	Dim uiws As New NotesUIWorkspace
	Dim adminDoc As NotesDocument
	Dim uidoc As NotesUIDocument
	Dim newUIDoc As NotesUIDocument
	Dim docColl As NotesDocumentCollection
	Dim acctDoc As NotesDocument
	Dim promptList() As String
	Dim acctRes As String
	Dim samlUNID As String
	Dim arrayIdx  As Integer
	Dim LinksItem As NotesItem
	Dim boxType As Long
	
	boxType&amp; = MB_OK + MB_ICONSTOP
	arrayIdx = 0
	
	Set uidoc = uiws.CurrentDocument
	Set adminDoc= uidoc.Document
	Set thisDB=session.CurrentDatabase
	
	Set LinksItem = adminDoc.GetFirstItem("$AccountLinkId")
	Set AccountVw  = thisDB.GetView(ALLTYPEACCOUNTDOCS)
	Call AccountVw.refresh
	
	Set docColl = AccountVw.GetAllDocumentsByKey("SAML-IdP", True)
	Set acctDoc = docColl.GetFirstDocument
	While Not acctDoc Is Nothing 
		Redim Preserve promptList(arrayIdx) As String
		promptList(arrayIdx) = acctDoc.GetItemValue("AccountName")(0)
		arrayIdx = arrayIdx + 1
		Set acctDoc = docColl.GetNextDocument(acctDoc)
	Wend
	
	acctRes = uiws.Prompt (PROMPT_OKCANCELLIST, PROMPT_TITLE,SELECT_ACCOUNTS_PROMPT,,promptList)
	If Not acctRes = "" Then
		Call adminDoc.ReplaceItemValue("samlAcctName", acctRes)
		Set AccountVw  = thisDB.GetView(ALLACCOUNTDOCS)
		Call AccountVw.refresh
		Set acctDoc = Accountvw.getdocumentbykey(acctRes, True)
		samlUNID = acctDoc.Universalid
		Call adminDoc.replaceitemvalue("SSOCREDENTIALID", samlUNID)
		
		adminDoc.saveoptions="0"
		admindoc.Form="Account"
		Call admindoc.ReplaceItemValue("$dtAccountsUpdate",Now)
		Call uidoc.Close(True)
		Set newUIDoc = uiws.EditDocument(True,adminDoc,False,,,True)
		Delete uiDoc
		Call newUIDoc.Document.ReplaceItemValue("SaveOptions","1")
	Else
		Print  PROCESS_CANCELLED
	End If
End Sub</lotusscript></code></globals><code event='windowtitle'><formula>@If(@IsNewDoc;"New Account";"Account: "+AccountName)</formula></code><code
