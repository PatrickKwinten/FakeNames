TypeOptions</formula></keywords><code event='inputtranslation'><formula>@If(@ThisValue="FileShare":"Connections";@SetField("IS_CSRFTOKEN_REQUIRED";"1");@True);
@If(@ThisValue="Connections";@SetField("AccountProtocol";"7");@ThisValue="Sametime";@SetField("AccountProtocol";"8"); @True);
@If(@ThisValue="Connections";@SetField("isPrimaryAccount";"1");@True);
@If(@ThisValue="SAML-IdP";@SetField("AccountProtocol";"8");@True);
@ThisValue</formula></code><code event='onchange'><lotusscript>Sub Onchange(Source As Field)
	Dim ws As New notesuiworkspace
	Dim doc As notesdocument
	Dim uidoc As NotesUIDocument
	
	Set uidoc = ws.CurrentDocument
