</lotusscript></code><code event='CreateDatabase'><lotusscript>Function CreateDatabase(strTemplateFile As String, strOutput As String, strTitle As String) As Integer
	Dim dbTemplate As NotesDatabase
	Dim dbOutput As NotesDatabase
	
	' Open up the template
	Set dbTemplate = New NotesDatabase("", strTemplateFile)
	If Not(dbTemplate.IsOpen) Then
		CreateDatabase = False
		Exit Function
	End If
	
	
	Set dbOutput = New NotesDatabase("", strOutput)
	If (dbOutput.IsOpen) Then          
		CreateDatabase = True
		Exit Function
	End If
	
	Set dbOutput = dbTemplate.CreateFromTemplate("", strOutput, True)
	dbOutput.Title=strTitle
	
	CreateDatabase = (dbOutput.IsOpen)
End Function
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
	Dim  doc As NotesDocument
	Dim s As New notessession
	Dim db As NotesDatabase
	Dim strServerName  As String
	Dim strConsoleRet1, strConsoleRet2 As String
	
	On Error Goto ERROR_HANDLER
	Set doc=source.Document	
	Set db = s.CurrentDatabase
	
	Forall v In doc.Items		
		If Ucase$(Right$(v.Name, 3)) = "$ED"  Then
			v.SaveToDisk = False
		End If
	End Forall
	
	sOriginalAccountProtocol=doc.GetItemValue("AccountProtocol")(0)
	sOriginalAccountServer=doc.GetItemValue("AccountServer")(0)
	If Source.IsNewDoc Then
		Call doc.ReplaceItemValue("AccountPFlag",doc.GetItemValue("AccountProtocol"))
		Call doc.ReplaceItemValue("AccountSFlag",doc.GetItemValue("SSLStatus"))
	Else
		sOriginalAccountType = doc.GetItemValue("AccountType")(0)
	End If
	
	If doc.GetItemValue("bLLN2")(0) = "" Then
		strServerName = db.Server
		strConsoleRet1 = s.SendConsoleCommand(strServerName, "show configuration $LLN2_FOR_PUBNAMES")
		Call doc.ReplaceItemValue("bLLN2", Val(Strright(strConsoleRet1, "=")))
		Call doc.ReplaceItemValue("IS_CLOUD_ACCOUNT", Val(Strright(strConsoleRet1, "=")))
	End If
	
	Call source.RefreshHideFormulas
	
	If source.EditMode = True Then
		Call source.refresh
	End If
	
ROUTINE_EXIT:
	Exit Sub
	
ERROR_HANDLER:
	Resume ROUTINE_EXIT
	
End Sub
</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	Dim was As New NotesUIWorkspace
	Dim uidoc As notesuidocument
	Dim  doc As NotesDocument
	Dim checkdoc As NotesDocument
	Dim ProtocolType As String
	Dim strProtocolString As String
	Dim bSuccess As Integer
	Dim vAccountName As Variant
	Dim strFileName As String
	Dim ws As New NotesUIWorkspace
	Dim vValid As Variant	
	Dim dc As NotesDocumentCollection
	Dim db As NotesDatabase
	Dim session As New NotesSession
	Dim pCounter1     As Integer     
	Dim pItemNames1() As String     'array of item names         
	Dim pRefItemName As String     'real item name refered by the ED item   
	Dim fieldName As String            
	Dim propItem As NotesItem
	
	Set uidoc = ws.Currentdocument
	Set doc = Source.Document
	If Not doc.HasItem("bLLN2") Then
		Call doc.replaceitemvalue("bLLN2", 0)
	End If
	
	If Not doc.GetItemValue("bLLN2")(0)=1 Then
		Set db = session.CurrentDatabase
		Set view = db.GetView("($Accounts)")
		Set uidoc=Source
		Set doc=source.Document	
		Call view.refresh
		Set dc= view.GetAllDocumentsByKey(doc.GetItemValue("AccountName")(0), True)
		
		On Error Goto RefreshError
		Call uidoc.refresh
		
		If dc.count &gt;0 Then
			Set checkdoc=dc.GetFirstDocument
			If checkdoc.NoteID &lt;&gt; doc.NoteID Then
				Messagebox "An Account with this name already exists.  Please choose a different Account name."
				Continue=False
				Exit Sub
			End If
		End If
		
		vValid=Evaluate(|@Contains(AccountName;"\\":"/":":":"+":"=":"*":"&lt;":"&gt;":"\"":";":"?":",":"[":"]":"{":"}":"\'":"(":")")|,doc)
		If vValid(0)=1 Then
			Call uidoc.refresh
			Continue=False
			Exit Sub
		Else 
			vValid=Evaluate({@Contains(AccountName;"|")},doc)
			If vValid(0)=1 Then
				Call uidoc.refresh
				Continue=False
				Exit Sub
			End If
		End If
		
		If doc.GetItemValue("AccountName")(0) = "" Then
			Call uidoc.refresh
			Exit Sub
		End If
		
	Else
		CalcAcctName
	End If
	vAccountName=Evaluate(|@ReplaceSubString(AccountName;" ":".";"")|,doc)
	
	strFileName=vAccountName(0)
	If Len(strFileName) &gt; 8 Then
		strFileName=Left(strFileName, 8)+".nsf"
	Else
		strFileName=strFileName+".nsf"
	End If
	
	strTitle=doc.GetItemValue("AccountName")(0)
	
	ProtocolType=doc.GetItemValue("AccountProtocol")(0)
	
	If doc.GetItemValue("AccountFileName")(0) &lt;&gt; "" Then
		If (ProtocolType &lt;&gt; sOriginalAccountProtocol)  And Not doc.IsNewNote Then 
			Set dbOutput = New NotesDatabase("",doc.GetItemValue("AccountFileName")(0))
			If dbOutput.IsOpen And ProtocolType &lt;&gt; "6" Then
				Call dbOutput.Remove
				Call doc.ReplaceItemValue("AccountFileName", "")
			End If
		End If	
	End If
	
	Select Case ProtocolType
	Case "2"
		If doc.GetItemValue("AccountFileName")(0) = "" Then 
			'Check to see if a db with the same name already exists if this is a new document.
			Set dbOutput = New NotesDatabase("", strFileName)
			If (doc.IsNewNote Or (ProtocolType &lt;&gt; sOriginalAccountProtocol)) And dbOutput.IsOpen Then
				vAccountName=Evaluate(|@Right(@Password(@Text(@Random));"(")|)
				strFileName=Left(vAccountName(0),8)+".nsf"
				Set dbOutput = New NotesDatabase("", strFileName)
				If dbOutput.IsOpen Then
					Messagebox "A proxy database cannot be created because a file with this name already exists.  Please change your Account name."
					Continue=False
					Exit Sub
				End If
			End If
			strProtocolString=" (IMAP)"
			strTitle=strTitle+strProtocolString
			
			bSuccess = CreateDatabase("imapcl5.ntf", strFileName, strTitle)
			If bSuccess = True Then		
				Call doc.ReplaceItemValue("AccountFileName", strFileName)
				Call doc.ReplaceItemValue("IMAPFirstOpen", "1")
				Call ws.AddDatabase("", strFileName)
			Else
				Print "Error: Mail (IMAP) template does not exist."
				Continue=False
			End If
		End If
	Case "3"
		If doc.GetItemValue("AccountFileName")(0) = "" Then
			'Check to see if a db with the same name already exists if this is a new document.
			Set dbOutput = New NotesDatabase("", strFileName)
			If (doc.IsNewNote Or (ProtocolType &lt;&gt; sOriginalAccountProtocol)) And dbOutput.IsOpen Then
				vAccountName=Evaluate(|@Right(@Password(@Text(@Random));"(")|)
				strFileName=Left(vAccountName(0),8)+".nsf"
				Set dbOutput = New NotesDatabase("", strFileName)
				If dbOutput.IsOpen Then
					Messagebox "A proxy database cannot be created because a file with this name already exists.  Please change your Account name."
					Continue=False
					Exit Sub
				End If
			End If
			strProtocolString=" (NNTP)"
			strTitle=strTitle+strProtocolString
			bSuccess = CreateDatabase("nntpcl6.ntf", strFileName, strTitle)
			If bSuccess = True Then
				Call doc.ReplaceItemValue("AccountFileName",strFileName)
				Call ws.AddDatabase("", strFileName)
			Else
				Print "Error: News Articles (NNTP) template does not exist."
				Continue=False
			End If
		End If
	End Select
	
	
	Forall item In doc.Items                                            
		If Ucase$(Right$(item.Name, 3)) = "$ED" Then                           
			If item.Values(0) = "1" Then                                          
				pRefItemName = Left$(item.name, Len(item.name) - 3)                                                                 
				Redim Preserve pItemNames1(pCounter1) As String                      
				pItemNames1(pCounter1) = pRefitemName                                                      
				pCounter1 = pCounter1 + 1         
			End If
		End If
	End Forall
	If doc.getitemvalue("useGlobalConnSettings$ED")(0) = "1" And doc.getitemvalue("connectionType$ED")(0) = "1" Then
		Redim Preserve pItemNames1(pCounter1+3) As String                      
		pItemNames1(pCounter1) = "proxyType"
		pCounter1 = pCounter1 + 1                   
		pItemNames1(pCounter1) = "proxyHost"
		pCounter1 = pCounter1 + 1                   
		pItemNames1(pCounter1) = "proxyPort"
		pCounter1 = pCounter1 + 1                   
		pItemNames1(pCounter1) = "proxyResolvesLocally"
	End If
	Call doc.ReplaceItemValue("$AcctEditableList", pItemNames1())
	
	Set propItem = doc.GetFirstItem("EditableList")                  
	If Not propItem Is Nothing Then                                         
		Forall v In propItem.Values   
			If Not v = "" Then
				Redim Preserve pItemNames1(pCounter1) As String                      
				pItemNames1(pCounter1) = v
				pCounter1 = pCounter1 + 1
			End If
		End Forall                                                            
	End If      
	
	If Not doc.GetItemValue("bLLN2")(0)=1 Then
 	    ' for on-premise always add username &amp; password fields to the $EditableList
		Redim Preserve pItemNames1(pCounter1 + 1) As String                      
		pItemNames1(pCounter1) = "AccountLoginName"                                                      
		pCounter1 = pCounter1 + 1         
		pItemNames1(pCounter1) = "AccountPassword"                                                      
		pCounter1 = pCounter1 + 1         
	End If
	
	
	Call doc.ReplaceItemValue("$EditableList",pItemNames1())   
	
			'added by srao
	Call doc.ReplaceItemValue("AccountPortNumber", 80)
	' if the account type is st (or Connections or Other) then set the iconFileName field if an icon was attached in stIcon field
	Call doc.ReplaceItemValue("AccountFile", 	doc.GetItemValue("$file")(0)  )
	
	Call doc.ReplaceItemValue("OriginalAccountProtocol",sOriginalAccountProtocol)
	Call doc.ReplaceItemValue("OriginalAccountServer",sOriginalAccountServer)
	If doc.GetItemValue("AccountPassword")(0) &lt;&gt; "" Then
		Set PkName = New NotesName(doc.GetItemValue("AccountAuthor")(0))
		Call doc.ReplaceItemValue("PublicEncryptionKeys", PkName.Abbreviated )
	End If
	
	'added by LBH
	If Not (source.IsNewDoc) Then  'Update datetime in security settings document if there is one that contains a link to this account doc
		Dim accountUNID(0) As String
		accountUNID(0) = source.document.UniversalID
		Call ProcessAcctNote(accountUNID,MODIFIED_ACCOUNT)
	End If
	Exit Sub
RefreshError:
	Continue=False
	Exit Sub
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	Dim propItem As NotesItem
	Dim fieldName As String
	Dim doc As NotesDocument
	
	If Not Source.IsNewDoc Then
		Set doc = Source.document
		
		Set propItem = doc.GetFirstItem("$AcctEditableList")                  
		If Not propItem Is Nothing Then                                         
			Forall v In propItem.Values   
				If Not v = "" Then
					fieldName = v &amp; "$ED"
					If (doc.HasItem(v) Or v = "UserName") Then
						Call doc.ReplaceItemValue(fieldName, "1")                         
					End If
				End If
			End Forall                                                            
		End If     
	End If
End Sub</lotusscript></code>
