<par def='17'><run><font size='9pt'/></run><run><font size='9pt'/><field usenotesstyle='false'
 height='0.2000in' width='99%' multiline='true' borderstyle='single' type='text'
 kind='editable' name='AccountServer' taborder='4' htmlid='AccountServer'><code
 event='inputvalidation'><formula>L_ERR_MSG1 := "You must enter a server name.";
L_ERR_MSG2 := "You must enter a properly formed URL.";

sMsg := @If(AccountLoginName != "" &amp; AccountPassword != "" &amp; AccountServer = ""; L_ERR_MSG1;  @ThisValue!="" &amp; AccountType!="" &amp; AccountType !="Sametime" &amp; AccountType!="FileShare" &amp; AccountType!="Connections" &amp; (!@Begins(@ThisValue;"http://") &amp; !@Begins(@ThisValue;"https://")); L_ERR_MSG2; "");

REM {return error message};
_tDb   := @ReplaceSubstring(@Subset(@DbName; -1); " ":"\\" ; "+":"/");
_tMsg  := @ReplaceSubstring(sMsg; "\\":"'":"\""; "%5C":"%27":"%22");
_tWeb1 := "&lt;script src='/" + _tDb + "/WebJSNavigationTools?OpenJavaScriptLibrary'&gt;&lt;/script&gt;";
_tWeb2 := "&lt;script&gt;ntValidationFailure('" + _tMsg + "');&lt;/script&gt;";
_tWeb  := _tWeb1 + _tWeb2;

@If(sMsg = "";
	@Success;
	@Failure(@If(@ClientType = "Notes"; sMsg; _tWeb)))</formula></code><code
 event='onchange'><lotusscript>Sub Onchange(Source As Field)
	
	Dim authSession  As JavaSession
	Dim authClass As JavaClass
	Dim authUrl As JavaObject
	Dim ws As New notesuiworkspace
	Dim doc As notesdocument
	Dim uidoc As NotesUIDocument
	Dim myauthUrl As String
	Dim myServer As String
	
	'Begin DNT
	' do nothing if this is lln2
	If bLLN2=1 Then 
		Return	
	End If
	
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	
	myServer = doc.GetItemValue("AccountServer")(0)
	If myServer = "" Then 
		Return
	End If
	
     ' update the authurl for connections 
	If doc.GetItemValue("AccountType")(0) = "Connections" Then 
		If doc.GetItemValue("authtype")(0) = "J2EE-FORM" Then 
			Set authSession = New JavaSession()
			Set authClass = authSession.GetClass("AccountsHelper")
			Set authUrlObject = authClass.CreateObject()
			myauthUrl = authUrlObject.getAuthServerUrl(myServer)
			
			Call doc.ReplaceItemValue("authserver", myauthUrl)
			uidoc.Refresh
			
		End If
		
	End If
	'END DNT
	
End Sub
</lotusscript></code></field></run><run><font size='9pt'/></run></par></tablecell>
