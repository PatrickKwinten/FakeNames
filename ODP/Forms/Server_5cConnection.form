	Dim s As New NotesSession
	Dim db As NotesDatabase
	Dim doc As NotesDocument
	Set db = s.CurrentDatabase
	Dim uidoc As notesuidocument
	Set uidoc=source
	Set doc = uidoc.document	
	Dim version As Variant
	version = Evaluate(|@Version|)
	
	If Cstr(version(0)) &lt; 163 Then
		Exit Sub
	End If
	
	If (doc.GetItemValue("RLAN2")(0) &lt;&gt; "") Or (doc.GetItemValue("NNTPPassword")(0) &lt;&gt; "") Then
		If db.Server = "" Then
			Set PkName = New NotesName(doc.GetItemValue("Source")(0))
			Call doc.ReplaceItemValue("PublicEncryptionKeys", PkName.Abbreviated )
		Else
'			Call doc.ReplaceItemValue("PublicEncryptionKeys",Evaluate(|@Name([Abbreviate];@Unique(Source:@ExpandNameList(@if(@Subset(@DbName;1) = "";@Subset(@MailDbName);@Subset(@DbName;1));Owner:LocalAdmin):@UserName))|,doc))
			Call doc.ReplaceItemValue("PublicEncryptionKeys",Evaluate(|@Name([Abbreviate];@Unique(Source:Owner:LocalAdmin:@UserName))|,doc))			
		End If
	End If
