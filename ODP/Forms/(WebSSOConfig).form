<action title='Keys...\Create Domino SSO Key' showinmenu='false' hide='preview read web'><imageref
 name='btnTool.gif'/><code event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	Dim secret As String
	Dim mode As String
	Dim dom As String
	Dim rsa As String
	Dim des As String
	
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	
	secret = String(LTPA_SECRETLEN,0)
	
	'Make sure the doc is not already initialized
	mode = uidoc.FieldGetText(MODEFIELD)
	dom = uidoc.FieldGetText(DOMSECRETFIELD)
	des = uidoc.FieldGetText(WS3DESDATAFIELD)
	rsa = uidoc.FieldGetText(WSRSADATAFIELD)
	
	If (mode &lt;&gt; "") Or (dom &lt;&gt; "") Or (des &lt;&gt; "") Or (rsa &lt;&gt; "") Then
		continue = Msgbox(MSGBOX_WARNTEXT,49,MSGBOX_WARNTITLE)
		If   continue &lt;&gt; 1 Then
			Exit Sub
		End If		
	End If
	
	'Call into nnotes to get a randomly generated domino secret
	Call LtpaGenerateSecret(secret$, LTPA_SECRETLEN)
	
	'Terminate the return string correctly
	secret = Left(secret$, Instr(secret$, Chr$(0)) -1)
	
	'Write the secret to the document
	Call doc.ReplaceItemValue(DOMSECRETFIELD,secret)
	
	'Make sure everything went as planned
	If doc.GetItemValue(DOMSECRETFIELD)(0) = "" Then
		Msgbox MSGBOX_GENERRORTEXT,MB_OK,MSGBOX_ERRORTITLE
	Else
		Call doc.ReplaceItemValue(MODEFIELD,"0")
		Msgbox MSGBOX_GENSUCCESSTEXT,MB_OK,MSGBOX_SUCCESSTITLE
	End If
	
	'Refresh the UI document
	Call uidoc.RefreshHideFormulas
	
End Sub</lotusscript></code></action>
<action title='Keys...\Import WebSphere LTPA Keys' icon='84' hide='preview read web'><code
 event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim ws As New NotesUIWorkspace
	Dim uidoc As NotesUIDocument
	Dim doc As NotesDocument
	
	Dim mode As String
	Dim dom As String
	Dim rsa As String
	Dim des As String
	
	Dim filename As String
	Dim password As String
	Dim desdata As String
	Dim rsadata As String
	Dim realm As String
	Dim version As Long
	
	Set uidoc = ws.CurrentDocument
	Set doc = uidoc.Document
	
	'Initialize our return values to the correct length
	desdata = String(LTPA_3DESDATALEN,0)
	rsadata = String(LTPA_RSADATALEN,0)
	realm      = String(LTPA_MAXWSREALMLEN,0)
	
	'Make sure the doc is not already initialized
	mode = uidoc.FieldGetText(MODEFIELD)
	dom = uidoc.FieldGetText(DOMSECRETFIELD)
	des = uidoc.FieldGetText(WS3DESDATAFIELD)
	rsa = uidoc.FieldGetText(WSRSADATAFIELD)
	
	If (mode &lt;&gt; "") Or (dom &lt;&gt; "") Or (des &lt;&gt; "") Or (rsa &lt;&gt; "") Then
		continue = Msgbox(MSGBOX_WARNTEXT,49,MSGBOX_WARNTITLE)
		If   continue &lt;&gt; 1 Then
			Exit Sub
		End If		
	End If
	
	'Get the import file path from the user. No LOCALBROWSE through LS.
	filename$ = Inputbox$(MSGBOX_FILETEXT,MSGBOX_FILETITLE)
	
	If filename = "" Then
		Msgbox MSGBOX_NOPATH, MSG_OK, MSGBOX_WARNTITLE
		Exit Sub
	End If
	
	'Get the import file password from the user
	password$ = ws.Prompt(PROMPT_PASSWORD,MSGBOX_PWTITLE,MSGBOX_PWTEXT)
	
	If password = "" Then
		Msgbox MSGBOX_NOPWD, MSG_OK, MSGBOX_WARNTITLE
		Exit Sub
	End If
	
	'Call into nnotes to get the data from the import file
	Call LtpaImportWSKeyFile(filename,password,desdata$,LTPA_3DESDATALEN,rsadata$,LTPA_RSADATALEN,realm$,LTPA_MAXWSREALMLEN,version)
	
	'Properly terminate the return values
	desdata = Left(desdata$, Instr(desdata$, Chr$(0)) -1)
	rsadata = Left(rsadata$, Instr(rsadata$, Chr$(0)) -1)
	realm = Left(realm$, Instr(realm$, Chr$(0)) -1)
	
	'Write the return information to the document
	Call doc.ReplaceItemValue(WS3DESDATAFIELD,desdata)
	Call doc.ReplaceItemValue(WSRSADATAFIELD,rsadata)
	Call doc.ReplaceItemValue(WSREALMFIELD,realm)
	Call doc.ReplaceItemValue(WSVERSIONFIELD,version)
	
	'Make sure everything went as planned
	If doc.GetItemValue(WS3DESDATAFIELD)(0) = "" Then
		Msgbox MSGBOX_IMPERRORTEXT,MB_OK,MSGBOX_ERRORTITLE
	Else
		Call doc.ReplaceItemValue(MODEFIELD,"1")
		Msgbox MSGBOX_IMPSUCCESSTEXT,MB_OK,MSGBOX_SUCCESSTITLE
	End If
	
	'Refresh the UI document
	Call uidoc.RefreshHideFormulas
	
End Sub</lotusscript></code></action>
