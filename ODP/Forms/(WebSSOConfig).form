	"&lt;script src='/" + _tDb + "/WebJSTools?OpenJavaScriptLibrary'&gt;&lt;/script&gt;
	 &lt;script src='/" + _tDb + "/WebJSNavigationTools?OpenJavaScriptLibrary'&gt;&lt;/script&gt;";

	"&lt;script src='/" + _tDb + "/WebJSNavigationTools5?OpenPage'&gt;&lt;/script&gt;"
)</formula></code><code event='AddTextListToItem'><lotusscript>Sub AddTextListToItem(intext As String, delim As String, item As NotesItem)
	
	Dim delimpos As Integer
	Dim strlen As Integer	
	Dim value As String
	Dim remainder As String
	
	curpos = 0
	strlen = Len(intext)
	remainder = intext
	
	If strlen = 0 Then
		Exit Sub
	End If
	
	Do While strlen &lt;&gt; 0 
		
		delimpos = Instr(remainder, delim)
		If delimpos = 0 Then
			value = remainder	
			remainder = ""
			strlen = 0
		Else			
			value = Left$ (remainder, delimpos-1)
			remainder = Right$ (remainder, strlen - delimpos - 1)
			strlen = Len(remainder)		
		End If
		
		If Len(value) &gt; 0 Then
			
			'Canonicalize the name
			Dim notesname As New NotesName(value)
			
			If Not item.Contains(notesname.Canonical) Then
				Call item.AppendToTextList(notesname.Canonical)			
			End If
		End If
		
	Loop
	
End Sub
</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Dim sess As New NotesSession
	Dim doc As Notesdocument
	Dim servers As String
	Dim owners As String
	Dim admins As String
	
	Dim mode As String
	Dim requireditem As String
	
	Dim domain As String
	
	On Error Goto errhandle
	
	Set doc = Source.Document
	
	'Make sure an "LtpaToken" configuration does not exist already
	Dim cfgname As String
	Dim cfgorg As String
	
	cfgname = Source.Fieldgettext(NAMEFIELD)
	cfgorg = Source.Fieldgettext(ORGFIELD)
	
	If (cfgorg = "" And cfgname = R5CONFIGNAME) Then
		
		Dim nabdb As NotesDatabase
		Dim ssoview As NotesView	
		Dim r5config As Notesdocument
		
		Set nabdb = sess.Currentdatabase
		Set ssoview = nabdb.Getview("($WebSSOConfigs)")
		
		Set r5config = ssoview.Getdocumentbykey(R5CONFIGNAME,1)
		
		If (Not r5config Is Nothing) Then
			
			If doc.Universalid &lt;&gt; r5config.Universalid Then
				Msgbox ERR_CONFIGURATION_EXISTS
				Goto errhandlenobox
			End If
			
		End If
		
	End If
	
	'Make sure the doc is initialized
	mode = Source.FieldGetText(MODEFIELD)
	
	If mode = "" Then
		Msgbox ERR_NOT_INITIALIZED
		Goto errhandlenobox
	End If
	
	If mode = "0" Then
		requireditem = Source.FieldGetText(DOMSECRETFIELD)
		If requireditem = "" Then
			Msgbox ERR_NOT_INITIALIZED
			Goto errhandlenobox
		End If
	Else
		requireditem = Source.FieldGetText(WSRSADATAFIELD)
		If requireditem = "" Then
			Msgbox ERR_NOT_INITIALIZED
			Goto errhandlenobox
		End If
		
		requireditem = Source.FieldGetText(WS3DESDATAFIELD)
		If requireditem = "" Then
			Msgbox ERR_NOT_INITIALIZED
			Goto errhandlenobox
		End If
	End If
	
	If doc.HasItem(PUBKEYFIELD) Then
		Call doc.RemoveItem(PUBKEYFIELD)
	End If
	
	Dim item As New NotesItem(doc, PUBKEYFIELD, "")
	
	'Add the current user to the item
	Call item.AppendToTextList(sess.Username)
	
	'Append the owners to the item
	owners = Source.FieldGetText(OWNERSFIELD)
	Call AddTextListToItem(owners, Chr(13), item)
	
	'Append the admins to the item
	admins = Source.FieldGetText(ADMINSFIELD)
	Call AddTextListToItem(admins, Chr(13), item)
	
	'Append the servers to the item
	servers = Source.FieldGetText(SERVERSFIELD)
	Call AddTextListToItem(servers, DELIM, item)
	
	domain = Source.Fieldgettext(DOMAINFIELD)
	
	'Make sure the domain is specified
	If (domain = "") Then
		Msgbox ERR_DOMAIN_REQUIRED
		Goto errhandlenobox
	End If
	
	'Make sure the Domain Item begins with a period
	If ("." &lt;&gt; String$(1,domain)) Then
		Call Source.Fieldsettext(DOMAINFIELD,"." + domain)
	End If
	
	Exit Sub
	
errhandle:
	Msgbox ERR_NO_SAVE &amp; ": " &amp; Error$,,"our error"
errhandlenobox:	
	Continue = False
	Exit Sub
	
End Sub</lotusscript></code>
