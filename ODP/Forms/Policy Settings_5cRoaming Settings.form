	If Continue = False Then
		Exit Sub
	End If
	
	Call PolSetQueryOpen(Source, Mode, Isnewdoc, Continue)
	
	Call SetOldDocNm(Source, Isnewdoc)
End Sub
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
