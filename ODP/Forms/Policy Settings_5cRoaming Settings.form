</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	' need to make sure we exit out of MailSystem field
	Call Source.GotoField("PlcyDescr")
	Dim Policy As NotesDocument
	Dim item As Notesitem
	Dim length As Long
	
	'Call source.Refresh
	Set Policy = Source.Document
	
	Set item = Policy.GetFirstItem("RoamStartDlg")
	length = item.valueLength
	
	If length &gt; 512 Then
		Continue = False
		Msgbox ROAMSTARTERROR, MB_OK+MB_ICONSTOP, ROAMTITLE
		Call Source.GotoField("RoamStartDlg")
		Exit Sub
	End If
	
	Set item = Policy.GetFirstItem("RoamFinishDlg")
	length = item.valueLength
	
	If length &gt; 512 Then
		Continue = False
		Msgbox ROAMFINISHERROR, MB_OK+MB_ICONSTOP, ROAMTITLE
		Call Source.GotoField("RoamFinishDlg")
		Exit Sub
	End If
	
	Call ComputePOItemList(Policy)
	Call ComputeIFPItemList(Policy)
	Call ComputeHAItemList(Policy)
	
	Call VerifyUnique(Source, Continue)
	
	Call Policy.Removeitem("$OnBehalfOf")
'	Call DontChangeItems(Source.document)	
	
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	Dim FromWhere As Integer
	FromWhere = 1
