</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	Dim Policy As NotesDocument
	
	Call Source.refresh   'this is needed so that the formula fields are calculated properly
	Set Policy = Source.Document
	
	If HasConflictingNSLSettings(Source.Document) Then
		If Msgbox(|You are saving security settings which may be conflicting.  Unless these settings will only be enforced by Domino 8.5 or higher servers, "Check password on Notes id file" set to "Yes" on the Password Management Basics tab conflicts with enabling Notes shared login on the Notes Shared Login tab.  Click "OK" to continue saving these settings or "Cancel" to resolve this conflict by setting "Check password on Notes id  file" to "No" or by disabling Notes shared login.|,MB_OKCANCEL+MB_ICONSTOP,"Settings Conflict") = IDOK Then
			Continue = True
		Else
			Continue = False
			Exit Sub			
		End If
		
	End If
	
	bSyntaxOk = True
	
	Set curitem = Policy.getFirstItem( "NSLOn$SF")
	If Policy.getItemValue( "NSLOnEntry")(0) &lt;&gt; "" Then
		If Not curitem.type = FORMULA Then
			Messagebox COMPILE_FAILURE_SAVE_BODY,  MB_ICONSTOP, COMPILE_FAILURE_TITLE 
			Source.goToField "NSLOnEntry"
			Continue = False
			bSyntaxOk = False
			Exit Sub
		End If
	End If
	
	Set curitem = Policy.getFirstItem( "NFLOn$SF")
	If Policy.getItemValue( "NFLOnEntry")(0) &lt;&gt; "" Then
		If Not curitem.type = FORMULA Then
			Messagebox COMPILE_FAILURE_SAVE_BODY,  MB_ICONSTOP, COMPILE_FAILURE_TITLE 
			Source.goToField "NFLOnEntry"
			Continue = False
			bSyntaxOk = False
			Exit Sub
		End If
	End If
	Call CleanPxyFields(Policy)
	
	Call ComputePOItemList(Policy)
	Call ComputeIFPItemList(Policy)
	Call ComputeHAItemList(Policy)
	Call VerifyUnique(Source, Continue)
	
	Call Policy.Removeitem("$OnBehalfOf")
	
End Sub
